{
  "name": "vinch",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:vinch": "react-scripts build",
    "build:storybook": "build-storybook -o build/storybook",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix",
    "release": "standard-version --no-verify",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "16.9.4",
    "firebase": "^7.6.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebase-hooks": "^2.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addon-storyshots": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/storybook-react-router": "^1.0.1",
    "@types/storybook__addon-storyshots": "^5.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "coveralls": "^3.0.9",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "mockdate": "^2.0.5",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "standard-version": "^6",
    "storybook-react-router": "^1.0.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/serviceWorker.ts",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/*.stories.*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
